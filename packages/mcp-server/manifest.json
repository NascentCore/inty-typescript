{
  "dxt_version": "0.1",
  "name": "inty-mcp",
  "version": "0.0.1",
  "description": "The official MCP Server for the Inty API",
  "author": {
    "name": "Inty",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stainless-sdks/inty-typescript.git"
  },
  "homepage": "https://github.com/stainless-sdks/inty-typescript/tree/main/packages/mcp-server#readme",
  "documentation": "",
  "server": {
    "type": "node",
    "entry_point": "${__dirname}/index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "INTY_API_KEY": "${user_config.INTY_API_KEY}"
      }
    }
  },
  "user_config": {
    "INTY_API_KEY": {
      "title": "api_key",
      "description": "\n            输入格式为: your_token\n            注意: 不需要Bearer前缀\n            ",
      "required": true,
      "type": "string"
    }
  },
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
